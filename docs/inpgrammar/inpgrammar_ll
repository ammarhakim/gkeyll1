/* This is a LL(1) version of the LALR(1) grammar in inpgrammar file.
 */

input_file : decl
;

decl : decl_top decl_list decl_end
;

decl_top : LEFT_ANGLE ID RIGHT_ANGLE
;

decl_end : LEFT_ANGLE_FRONT_SLASH ID RIGHT_ANGLE
;

decl_list : decl_value decl_list_rest
;

decl_list_rest :  decl_value decl_list_rest
| <NULL>
;

decl_value : ID EQUAL value_list
| decl
;

value_list : LEFT_BOX value_sub_list RIGHT_BOX
| value
;

value_sub_list : value value_sub_list_rest
;

value_sub_list_rest : COMMA value value_sub_list_rest
| <NULL>
;

value : INT | REAL | STRING | VALUE | ID
;
