input_file : decl
;

decl : decl_top decl_list decl_end
;

decl_top : LEFT_ANGLE ID RIGHT_ANGLE
;

decl_end : LEFT_ANGLE_FRONT_SLASH ID RIGHT_ANGLE
;

decl_list : decl_value
| decl_list decl_value
;

decl_value : key_value | decl
;

key_value : ID EQUAL value_list
;

value_list : value
| LEFT_BOX value_sub_list RIGHT_BOX
;

value_sub_list : value
| value_sub_list COMMA value
;

value : INT | REAL | STRING | VALUE | ID
;
